{
    "parser": "@typescript-eslint/parser",
    "env": {
        "node": true
    },
    "root": true,
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
        "@typescript-eslint",
        "todo-ddl"
    ],
    "rules": {
        "@typescript-eslint/no-explicit-any": [
            "off"
        ],
        "@typescript-eslint/explicit-module-boundary-types": [
            "off"
        ],
        "@typescript-eslint/no-unused-vars": [
            "off"
        ],
        "todo-ddl/diy": [
            "warn"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "semi": [
            "warn",
            "never"
        ]
    }
}